{
  "name": "wms-admin",
  "version": "0.0.1",
  "description": "仓储系统",
  "author": "xinyirun",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve && webpack-dev-server --open",
    "build:stage": "vue-cli-service build --mode staging",
    "build:test1": "vue-cli-service build --mode test1",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "test:unit:playwright": "playwright test --project=unit-tests",
    "test:integration": "playwright test --project=integration-tests",
    "test:e2e": "playwright test --project=e2e-tests",
    "test:e2e:ui": "playwright test --project=e2e-tests --ui",
    "test:e2e:debug": "playwright test --project=e2e-tests --debug",
    "test:e2e:headed": "playwright test --project=e2e-tests --headed",
    "test:playwright:all": "playwright test",
    "test:playwright:report": "playwright show-report",
    "test:playwright:install": "playwright install",
    "test:all": "npm run test:ci && npm run test:unit:playwright && npm run test:integration && npm run test:e2e",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PanJiaChen/vue-element-admin.git"
  },
  "bugs": {
    "url": "https://github.com/PanJiaChen/vue-element-admin/issues"
  },
  "eslitenConfig": {
    "rules": {
      "vue/no-unused-components": "off"
    }
  },
